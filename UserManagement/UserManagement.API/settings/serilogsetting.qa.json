{
    "Serilog": {
        "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.MongoDB"],
        "MinimumLevel": {
        "Default": "Warning",
          "Override": {
            "Microsoft": "Warning",
            "System": "Warning"
          }
        },
        "WriteTo": [
          {
            "Name": "MongoDB",
            "Args": {
              "databaseUrl": "mongodb://192.168.1.130:27017/Bannari",
              "collectionName": "ApplicationLogs",
              "restrictedToMinimumLevel": "Warning"
            }
          }
        ],
        "Enrich": ["FromLogContext"]
      }
  }  